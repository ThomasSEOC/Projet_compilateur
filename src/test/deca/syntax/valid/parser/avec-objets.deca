class A extends B{
    protected int x,y;

    public A(){
        this.x = x;
        this.y = y;
    }

    int getX(){
        return this.x;
    }
    int getY(){
        return this.y;
    }
    void setX(int x){
        this.x = x;
    }
    void setY(int y){
        this.y = y;
    }
}

{
    A a = new A();
    a.setX(2);
    a.setY(3);
    println("a.getX() = " ,a.getX());
    println("a.getY() = " ,a.getY());
}